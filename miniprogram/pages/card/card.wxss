/* pages/card.wxss */
/* iPhone 6尺寸作为设计稿基准 */
@import '/utils/animate.min.wxss';

.show {
  opacity: 1 !important;
}

image{
  width: 100%;
  height: 100%;
}

#music {
  position: fixed;
  z-index: 10;
  right: 70rpx;
  top: 70rpx;
}

#music button {
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  border: none;
  background-color: transparent;
  z-index: 11;
}

#music button::after {
  border: none;
}

.musicOn,
.musicOff {
  width: 80rpx;
  height: 80rpx;
}

.rotate {
  animation: rotate 3s linear infinite;
}

@keyframes rotate {
  from {
    transform: rotate(0deg)
  }

  to {
    transform: rotate(359deg)
  }
}

.bg {
  width: 100%;
  height: 100%;
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bg-1 {
  margin: 0 auto;
  top: 3%;
  height: 94%;
  width: 90%;
  opacity: 0;
}

.page {
  position: absolute;
  height: 94%;
  width: 90%;
  top: 3%;
  left: 5%;
  overflow: hidden;
}

.page .left,
.page .right,
.page .top,
.page .button {
  opacity: 0;
}

.page .left {
  position: absolute;
  width: 30rpx;
  height: 100%;
  left: 0;
  background-image:url('data:image/jpeg;base64,');
}

.page .right {
  width: 30rpx;
  height: 100%;
  right: 0;
  position: absolute;
  background-image:url('data:image/jpeg;base64,');
}

.page .top {
  height: 30rpx;
