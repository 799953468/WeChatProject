/* miniprogram/pages/index/index.wxss */
.info {
    position: relative;
    width: 90vw;
    height: 30vh;
    border-radius: 15px;
    background: #fff;
    margin: 0 auto;
    margin-bottom: 10px;
}

.user_img {
    display: inline-flex;
    margin-top: 20rpx;
    margin-left: 20rpx;
    width: 20vw;
    height: 20vw;
}

.user_info {
    position: absolute;
    top: 4vh;
    left: 25vw;
}

.user_info .user_name {
    font-size: 40rpx;
    font-weight: bold;
    flex-wrap: wrap;
}

.user_info .user_tel {
    font-size: 24rpx;
    margin-top: 5px;
    font-weight: bold;
}

.user_list {
    display: flex;
    flex-direction: row;
    justify-content: center;
    text-align: center;
    margin-top: 10%;
}

.user_list .item {
    width: 35vw;
    border-left: 1px solid #9a9a9a;
}

.user_list .item:nth-of-type(1) {
    border: none;
}

.user_list .item span {
    display: block;
}

.user_list .item span:nth-of-type(1) {
    font-weight: bold;
}

.user_list .item span:nth-of-type(2) {
    font-size: 24rpx;
    color: #9c9c9c;
}

.info .more::after {
    top: 20%;
    right: 0;
}

.more::after {
    content: '';
    background-size: 50%;
    background-repeat: no-repeat;
    position: absolute;
    width: 30px;
    height: 30px;
    background-image: url(data:image/png;base64,);
}

.user_setting {
    position: relative;
    width: 90vw;
    border-radius: 15px;
    background: #fff;
    margin: 10px auto;
}

.user_setting .item {
    position: relative;
    height: 6vh;
    width: 80vw;
    border-bottom: 1px solid #f4f4f4;
    margin: 15px auto 0 auto;
    padding-bottom: 5px;
}

.user_setting .item span {
    margin-left: 20px;
    font-size: 30rpx;
    font-weight: bold;
}

.user_setting .item:last-of-type {
    border: none;
}

.user_setting .item .more::after{
    top: 5px;
    right: 0;
}

.user_set::before{
    content: '';
    position: absolute;
    top: 1px;
    left: -5px;
    width: 200px;
    height: 200px;
    background-size: 10%;
    background-repeat: no-repeat;
    background-image: url(data:image/png;base64,);
}

.remind::before{
    content: '';
    width: 200px;
    height: 200px;
    position: absolute;
    top: 1px;
    left: -5px;
    background-size: 10%;
    background-repeat: no-repeat;
    background-image: url(data:image/png;base64,);
}

.setting {
    position: relative;
    width: 90vw;
    border-radius: 15px;
    background: #fff;
    margin: 10px auto;
}

.setting .item {
    position: relative;
    height: 6vh;
    width: 80vw;
    border-bottom: 1px solid #f4f4f4;
    margin: 15px auto 0 auto;
}

.setting .item span {
    font-size: 30rpx;
    font-weight: bold;
}

.setting .item:last-of-type {
    border: none;
}

.setting .item .more::after{
    top: 5px;
    right: 0;
}

.btn-hover{
    border: none;
}